# this file is automatically generated by CMake from
# sawOpenIGTLinkConfig.cmake.in

set (sawOpenIGTLink_INCLUDE_DIR "@sawOpenIGTLink_INCLUDE_DIR@")
set (sawOpenIGTLink_LIBRARY_DIR "@sawOpenIGTLink_LIBRARY_DIR@")
set (sawOpenIGTLink_LIBRARIES   "@sawOpenIGTLink_LIBRARIES@")

# figure out if this file is where it was configured (build tree) or
# not (installed tree)
set (sawOpenIGTLink_CMAKE_CONFIG_FILE "@sawOpenIGTLink_CMAKE_CONFIG_FILE@")
get_filename_component (THIS_FILE_PATH
                        ${CMAKE_CURRENT_LIST_FILE}
                        REALPATH)

if ("${sawOpenIGTLink_CMAKE_CONFIG_FILE}" STREQUAL "${THIS_FILE_PATH}")
  # we're using the build directories
  set (sawOpenIGTLink_INCLUDE_DIR "@sawOpenIGTLink_INCLUDE_DIR@")
  set (sawOpenIGTLink_LIBRARY_DIR "@sawOpenIGTLink_LIBRARY_DIR@")
else ()
  # try to find the install dir, we know the install is using
  # share/sawOpenIGTLink so we can go ../..
  set (RELATIVE_INSTALL_DIR "${THIS_FILE_PATH}/../..")
  get_filename_component (ABSOLUTE_INSTALL_DIR "${RELATIVE_INSTALL_DIR}" ABSOLUTE)
  # set directories using the install dir
  set (sawOpenIGTLink_INCLUDE_DIR "${ABSOLUTE_INSTALL_DIR}/include")
  set (sawOpenIGTLink_LIBRARY_DIR "${ABSOLUTE_INSTALL_DIR}/lib")
endif ()

set (sawOpenIGTLink_LIBRARIES   "@sawOpenIGTLink_LIBRARIES@")
